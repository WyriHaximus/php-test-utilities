{
  "name": "wyrihaximus/test-utilities",
  "description": "\ud83d\udee0\ufe0f Test utilities for api-clients packages",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ergebnis/composer-normalize": "^2.29.0",
    "icanhazstring/composer-unused": "^0.8.7",
    "infection/infection": "^0.26.16",
    "jakobbuis/simple-slow-test-reporter": "^1.0",
    "maglnet/composer-require-checker": "^4.5.0",
    "nunomaduro/collision": "^7.1.0",
    "orklah/psalm-insane-comparison": "^2.2.0",
    "php-coveralls/php-coveralls": "^2.5.3",
    "php-parallel-lint/php-console-highlighter": "^1.0",
    "php-parallel-lint/php-parallel-lint": "^1.3.2",
    "php-standard-library/psalm-plugin": "^2.2.1 || ^1.1.5",
    "phpspec/prophecy": "^1.17",
    "phpspec/prophecy-phpunit": "^2.0.1",
    "phpstan/phpstan": "^1.9.2",
    "phpunit/phpunit": "^9.6.5",
    "psalm/plugin-phpunit": "^0.18.4",
    "roave/backward-compatibility-check": "^8.3.0",
    "roave/infection-static-analysis-plugin": "^1.28.0",
    "squizlabs/php_codesniffer": "^3.7.2",
    "thecodingmachine/safe": "^2.4.0",
    "vimeo/psalm": "^5.6.0",
    "wyrihaximus/coding-standard": "^2.12.0",
    "wyrihaximus/phpstan-rules-wrapper": "^2.9.0"
  },
  "conflict": {
    "symfony/dependency-injection": "<5.0.0"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\TestUtilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\TestUtilities\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "composer-unused/composer-unused-plugin": true
    },
    "platform": {
      "php": "8.2.13"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "ergebnis/composer-normalize",
      "icanhazstring/composer-unused",
      "infection/infection",
      "jakobbuis/simple-slow-test-reporter",
      "maglnet/composer-require-checker",
      "nunomaduro/collision",
      "orklah/psalm-insane-comparison",
      "php-coveralls/php-coveralls",
      "php-parallel-lint/php-parallel-lint",
      "php-standard-library/psalm-plugin",
      "phpstan/phpstan",
      "psalm/plugin-phpunit",
      "roave/backward-compatibility-check",
      "roave/infection-static-analysis-plugin",
      "squizlabs/php_codesniffer",
      "vimeo/psalm",
      "wyrihaximus/coding-standard",
      "wyrihaximus/phpstan-rules-wrapper"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
